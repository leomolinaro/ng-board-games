{
  "name": "ng-board-games",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/ng-board-games/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "options": {
        "outputPath": "dist/apps/ng-board-games",
        "index": "apps/ng-board-games/src/index.html",
        "browser": "apps/ng-board-games/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/ng-board-games/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "stylePreprocessorOptions": {
          "includePaths": [
            "apps/ng-board-games/src/styles",
            "apps/ng-board-games/src/app/barony/styles",
            "apps/ng-board-games/src/app/britannia/styles"
          ]
        },
        "assets": [
          "apps/ng-board-games/src/favicon.ico",
          "apps/ng-board-games/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/ng-board-games/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/ng-board-games/src/environments/environment.ts",
              "with": "apps/ng-board-games/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "ng-board-games:build:production"
        },
        "development": {
          "buildTarget": "ng-board-games:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "ng-board-games:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": ["zone.js", "zone.js/testing"],
        "tsConfig": "apps/ng-board-games/tsconfig.spec.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/ng-board-games/src/favicon.ico",
          "apps/ng-board-games/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/ng-board-games/src/styles.scss"
        ],
        "scripts": []
      }
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {}
    }
  }
}
